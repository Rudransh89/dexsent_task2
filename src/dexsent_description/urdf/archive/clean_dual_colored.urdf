
<?xml version="1.0"?>
<robot name="dual_crx_system">
  <material name="red">
    <color rgba="1 0 0 1"/>
  </material>


  <link name="world"/>

  <joint name="left_mount" type="fixed">
    <parent link="world"/>
    <child link="left_base_link"/>
    <origin xyz="0 0.5 0" rpy="0 0.785 0"/>
  </joint>

  <link name="left_base_link"><visual><geometry><box size="0.2 0.2 0.2"/></geometry><material name="red"/></visual></link>

  <joint name="left_joint_1" type="revolute">
    <parent link="left_base_link"/>
    <child link="left_link_1"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 0.2"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="left_link_1"><visual><geometry><box size="0.1 0.1 0.3"/></geometry><material name="red"/></visual></link>

  <joint name="left_joint_2" type="revolute">
    <parent link="left_link_1"/>
    <child link="left_link_2"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0 0 0.3"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="left_link_2"><visual><geometry><box size="0.1 0.1 0.4"/></geometry><material name="red"/></visual></link>

  <joint name="left_joint_3" type="revolute">
    <parent link="left_link_2"/>
    <child link="left_link_3"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0 0 0.4"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="left_link_3"><visual><geometry><box size="0.1 0.1 0.3"/></geometry><material name="red"/></visual></link>

  <joint name="left_joint_4" type="revolute">
    <parent link="left_link_3"/>
    <child link="left_link_4"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 0.3"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="left_link_4"><visual><geometry><box size="0.08 0.08 0.2"/></geometry><material name="red"/></visual></link>

  <joint name="left_joint_5" type="revolute">
    <parent link="left_link_4"/>
    <child link="left_link_5"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.2 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="left_link_5"><visual><geometry><box size="0.08 0.08 0.1"/></geometry><material name="red"/></visual></link>

  <joint name="left_joint_6" type="revolute">
    <parent link="left_link_5"/>
    <child link="left_tool0"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0.1 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="left_tool0"><visual><geometry><box size="0.05 0.05 0.05"/></geometry><material name="red"/></visual></link>


  <joint name="right_mount" type="fixed">
    <parent link="world"/>
    <child link="right_base_link"/>
    <origin xyz="0 -0.5 0" rpy="0 -0.785 0"/>
  </joint>

  <link name="right_base_link"><visual><geometry><box size="0.2 0.2 0.2"/></geometry><material name="red"/></visual></link>

  <joint name="right_joint_1" type="revolute">
    <parent link="right_base_link"/>
    <child link="right_link_1"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0 0 0.2"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="right_link_1"><visual><geometry><box size="0.1 0.1 0.3"/></geometry><material name="red"/></visual></link>

  <joint name="right_joint_2" type="revolute">
    <parent link="right_link_1"/>
    <child link="right_link_2"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0 0 0.3"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="right_link_2"><visual><geometry><box size="0.1 0.1 0.4"/></geometry><material name="red"/></visual></link>

  <joint name="right_joint_3" type="revolute">
    <parent link="right_link_2"/>
    <child link="right_link_3"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0 0 0.4"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="right_link_3"><visual><geometry><box size="0.1 0.1 0.3"/></geometry><material name="red"/></visual></link>

  <joint name="right_joint_4" type="revolute">
    <parent link="right_link_3"/>
    <child link="right_link_4"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0 0 0.3"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="right_link_4"><visual><geometry><box size="0.08 0.08 0.2"/></geometry><material name="red"/></visual></link>

  <joint name="right_joint_5" type="revolute">
    <parent link="right_link_4"/>
    <child link="right_link_5"/>
    <axis xyz="0 1 0"/>
    <origin xyz="0.2 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="right_link_5"><visual><geometry><box size="0.08 0.08 0.1"/></geometry><material name="red"/></visual></link>

  <joint name="right_joint_6" type="revolute">
    <parent link="right_link_5"/>
    <child link="right_tool0"/>
    <axis xyz="1 0 0"/>
    <origin xyz="0.1 0 0"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="2.0"/>
  </joint>
  <link name="right_tool0"><visual><geometry><box size="0.05 0.05 0.05"/></geometry><material name="red"/></visual></link>

  <ros2_control name="DualArmSystem" type="system">
    <hardware>
      <plugin>mock_components/GenericSystem</plugin>
    </hardware>
    <joint name="left_joint_1"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="left_joint_2"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="left_joint_3"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="left_joint_4"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="left_joint_5"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="left_joint_6"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    
    <joint name="right_joint_1"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="right_joint_2"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="right_joint_3"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="right_joint_4"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="right_joint_5"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
    <joint name="right_joint_6"><command_interface name="velocity"/><state_interface name="position"/><state_interface name="velocity"/></joint>
  </ros2_control>

</robot>